{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "flashblocks/timeline-story",
	"version": "0.1.0",
	"title": "Timeline Story",
	"category": "design",
	"icon": "marker",
	"description": "A single story entry within a timeline.",
	"keywords": [
		"story",
		"event",
		"milestone"
	],
	"parent": [
		"flashblocks/timeline"
	],
	"supports": {
		"html": false
	},
	"attributes": {
		"date": {
			"type": "string",
			"default": ""
		},
		"iconSvg": {
			"type": "string",
			"default": ""
		}
	},
	"usesContext": [
		"flashblocks/timeline-lineColor",
		"flashblocks/timeline-dotColor"
	],
	"textdomain": "flashblocks-timeline",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}